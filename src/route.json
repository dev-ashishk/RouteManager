{
    "api1": {
        "get": {
            "client": {
                "url": "/api/any-service",
                "requiredParams": {
                    "query": ["a", "b", "c"]
                },
                "express": {
                    "controllers": ["controller.hello","controller.sayHello"]
                }
            },
            "server": "/api/any-service/abcdef/:x/:y"
        },
        "post":{
            "client": {
                "url": "/api/any-service/:v/helloworld/:w",
                "requiredParams": {
                    "query": ["a", "b", "c"]
                },
                "express": {
                    "controllers": ["controller.returnParams"]
                }
            },
            "server": "/api/any-service/abcdef/:abc/:xyz"
        }
    }
}